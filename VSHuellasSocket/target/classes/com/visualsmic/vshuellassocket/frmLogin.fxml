<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="313.0" prefWidth="328.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visualsmic.vshuellassocket.LoginController">
   <children>
      <AnchorPane prefHeight="136.0" prefWidth="288.0">
         <children>
            <Label text="Login" />
         </children>
      </AnchorPane>
      <TextField fx:id="txtUsuario" promptText="Ingreses Usuario" />
      <PasswordField fx:id="txtPassWord" />
      <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#btnLoginOnAction" text="Ingresar" />
      <Button fx:id="btnLogout" layoutX="151.0" layoutY="277.0" mnemonicParsing="false" onAction="#btnLogoutOnAction" text="Salir" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
